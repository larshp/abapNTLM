{
  "input_folder": "downport",
  "input_filter": [".clas.", ".intf."],
  "output_folder": "output",
  "lib": "https://github.com/open-abap/open-abap",
  "write_unit_tests": true,
  "options": {
    "ignoreSyntaxCheck": false,
    "addFilenames": true,
    "addCommonJS": true,
    "unknownTypes": "runtimeError",
    "skip": [
      {"object": "ZCL_DES", "class": "lcl_test", "method": "encrypt", "note": "more unit tests, https://github.com/larshp/abapNTLM/issues/21"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test1", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test2", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test3", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test4", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test5", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test6", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test7", "note": "?"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "barrel", "note": "calls overflow() method"},
      {"object": "ZCL_MD4", "class": "lcl_test", "method": "test_overflow", "note": "float to float bug, fixed in next transpiler release"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "type_3_build", "note": "calls DES"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "lmv1_response", "note": "calls DES"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "ntlmv1_response", "note": "calls DES"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "lmv2_response_1", "note": "?"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "lmv2_response_2", "note": "?"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "ntlmv2_response_1", "note": "?"},
      {"object": "ZCL_NTLM", "class": "lcl_test", "method": "ntlm2_session_r", "note": "CALCULATE_HASH_FOR_RAW is not a function"},
      {"object": "ZCL_NTLM", "class": "lcl_convert_test", "method": "bit64_1", "note": "DB02_BLID not found, lookup"},
      {"object": "ZCL_NTLM", "class": "lcl_convert_test", "method": "bit64_2", "note": "DB02_BLID not found, lookup"},
      {"object": "ZCL_NTLM", "class": "lcl_convert_test", "method": "bit64_3", "note": "DB02_BLID not found, lookup"},
      {"object": "ZCL_NTLM", "class": "lcl_convert_test", "method": "bit64_4", "note": "DB02_BLID not found, lookup"},
      {"object": "ZCL_NTLM", "class": "lcl_convert_test", "method": "bit64_5", "note": "DB02_BLID not found, lookup"}
    ]
  }
}