{
  "input_folder": "downport",
  "input_filter": [".clas.", ".intf."],
  "output_folder": "output",
  "lib": "https://github.com/open-abap/open-abap",
  "write_unit_tests": true,
  "options": {
    "ignoreSyntaxCheck": false,
    "addFilenames": true,
    "addCommonJS": true,
    "unknownTypes": "runtimeError",
    "skip": [
      {"object": "ZCL_DES", "class": "lcl_test", "method": "parity_adjust", "note": "GET BIT error, fixed in next transpiler release"},
      {"object": "ZCL_DES", "class": "lcl_test", "method": "encrypt", "note": "probably GET BIT"}
    ]
  }
}